{
  "kind": "collectionType",
  "collectionName": "blog_categories",
  "info": {
    "singularName": "blog-category",
    "pluralName": "blog-categories",
    "displayName": "blog_category"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "text"
    },
    "blogs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog.blog",
      "mappedBy": "blog_categories"
    }
  }
}
